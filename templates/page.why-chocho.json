        "text_MtmQ88": {
          "type": "text",
          "name": "t:names.heading",
          "settings": {
            "text": "<p>From the Andes to our kitchen to strengthen the whole family</p><p></p>",
            "width": "fit-content",
            "max_width": "normal",
            "alignment": "left",
            "type_preset": "h2",
            "font": "var(--font-body--family)",
            "font_size": "1rem",
            "line_height": "normal",
            "letter_spacing": "normal",
            "case": "none",
