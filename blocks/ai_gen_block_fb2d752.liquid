{% doc %}
  @prompt
    Sección con título, párrafo y botón con imagen flotante que gira con scroll
    - Imagen posicionada al borde izquierdo o derecho
    - Centrada verticalmente respecto al contenedor
    - Optimizado para móvil (ocultar o reducir imagen)
    - Rotación suave con scroll
{% enddoc %}

{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-floating-section-{{ ai_gen_id }} {
    --fs-bg: {{ block.settings.background_color }};
    --fs-title-color: {{ block.settings.title_color }};
    --fs-title-size: {{ block.settings.title_size }}px;
    --fs-paragraph-color: {{ block.settings.paragraph_color }};
    --fs-paragraph-size: {{ block.settings.paragraph_size }}px;
    --fs-btn-bg: {{ block.settings.button_background_color }};
    --fs-btn-color: {{ block.settings.button_text_color }};
    --fs-btn-hover-bg: {{ block.settings.button_hover_background_color }};
    --fs-btn-hover-color: {{ block.settings.button_hover_text_color }};
    --fs-image-size: {{ block.settings.image_size }}px;

    position: relative;
    width: 100%;
    min-height: {{ block.settings.min_height }}px;
    padding: {{ block.settings.section_padding_top }}px 0 {{ block.settings.section_padding_bottom }}px;
    background-color: var(--fs-bg);
    overflow: visible;
    z-index: -1;
    display: flex;
    align-items: center;
  }

  /* Container */
  .ai-floating-section__container-{{ ai_gen_id }} {
    width: 100%;
    max-width: {{ block.settings.container_max_width }}px;
    margin: 0 auto;
    padding: 0 {{ block.settings.container_padding }}px;
    position: relative;
    z-index: 2;
  }

  /* Content */
  .ai-floating-section__content-{{ ai_gen_id }} {
    max-width: {{ block.settings.content_max_width }}px;
    
    {% if block.settings.content_alignment == 'center' %}
    margin: 0 auto;
    text-align: center;
    {% elsif block.settings.content_alignment == 'right' %}
    margin-left: auto;
    margin-right: 0;
    text-align: right;
    {% else %}
    margin-right: auto;
    margin-left: 0;
    text-align: left;
    {% endif %}
  }

  /* Title */
  .ai-floating-section__title-{{ ai_gen_id }} {
    color: var(--fs-title-color);
    font-size: var(--fs-title-size);
    font-weight: {{ block.settings.title_weight }};
    line-height: {{ block.settings.title_line_height }};
    margin: 0 0 {{ block.settings.title_margin_bottom }}px 0;
    {% if block.settings.title_uppercase %}
    text-transform: uppercase;
    letter-spacing: 0.02em;
    {% endif %}
  }

  .ai-floating-section__title-{{ ai_gen_id }} p {
    margin: 0;
  }

  /* Paragraph */
  .ai-floating-section__paragraph-{{ ai_gen_id }} {
    color: var(--fs-paragraph-color);
    font-size: var(--fs-paragraph-size);
    font-weight: {{ block.settings.paragraph_weight }};
    line-height: {{ block.settings.paragraph_line_height }};
    margin: 0 0 {{ block.settings.paragraph_margin_bottom }}px 0;
  }

  .ai-floating-section__paragraph-{{ ai_gen_id }} p {
    margin: 0 0 1em 0;
  }

  .ai-floating-section__paragraph-{{ ai_gen_id }} p:last-child {
    margin-bottom: 0;
  }

  /* Button */
  .ai-floating-section__button-{{ ai_gen_id }} {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: 8px;
    padding: {{ block.settings.button_padding_y }}px {{ block.settings.button_padding_x }}px;
    background-color: var(--fs-btn-bg);
    color: var(--fs-btn-color);
    font-size: {{ block.settings.button_font_size }}px;
    font-weight: {{ block.settings.button_font_weight }};
    text-decoration: none;
    border: {{ block.settings.button_border_width }}px solid {{ block.settings.button_border_color }};
    border-radius: {{ block.settings.button_border_radius }}px;
    cursor: pointer;
    transition: all 0.3s ease;
  }

  .ai-floating-section__button-{{ ai_gen_id }}:hover {
    background-color: var(--fs-btn-hover-bg);
    color: var(--fs-btn-hover-color);
    border-color: {{ block.settings.button_hover_border_color }};
  }

  /* ===== FLOATING IMAGE ===== */
  .ai-floating-section__image-{{ ai_gen_id }} {
    position: absolute;
    width: var(--fs-image-size);
    height: var(--fs-image-size);
    z-index: 1;
    will-change: transform;
    pointer-events: none;
    
    /* Centrado vertical respecto al contenedor */
    top: 50%;
    
    /* Posición horizontal */
    {% if block.settings.image_position == 'left' %}
    left: 0;
    transform: translateY(-50%) translateX(-{{ block.settings.image_offset }}%);
    {% else %}
    right: 0;
    transform: translateY(-50%) translateX({{ block.settings.image_offset }}%);
    {% endif %}
  }

  .ai-floating-section__image-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    border-radius: 50%;
    {% if block.settings.image_shadow %}
    box-shadow: 0 10px 40px rgba(0, 0, 0, 0.15);
    {% endif %}
  }

  .ai-floating-section__image-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background: linear-gradient(135deg, #f5f5f5 0%, #e0e0e0 100%);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-floating-section__image-placeholder-{{ ai_gen_id }} svg {
    width: 40%;
    height: 40%;
    opacity: 0.3;
  }

  /* ===== RESPONSIVE ===== */
  @media screen and (max-width: 991px) {
    .ai-floating-section__image-{{ ai_gen_id }} {
      --fs-image-size: {{ block.settings.image_size_tablet }}px;
    }
  }

  @media screen and (max-width: 749px) {
    .ai-floating-section-{{ ai_gen_id }} {
      min-height: auto;
      padding: {{ block.settings.section_padding_top_mobile }}px 0 {{ block.settings.section_padding_bottom_mobile }}px;
    }

    .ai-floating-section__container-{{ ai_gen_id }} {
      padding: 0 {{ block.settings.container_padding_mobile }}px;
    }

    .ai-floating-section__content-{{ ai_gen_id }} {
      max-width: 100%;
      {% if block.settings.content_alignment_mobile == 'center' %}
      text-align: center;
      margin: 0 auto;
      {% elsif block.settings.content_alignment_mobile == 'left' %}
      text-align: left;
      margin: 0;
      {% endif %}
    }

    .ai-floating-section__title-{{ ai_gen_id }} {
      font-size: {{ block.settings.title_size_mobile }}px;
      margin-bottom: {{ block.settings.title_margin_bottom_mobile }}px;
    }

    .ai-floating-section__paragraph-{{ ai_gen_id }} {
      font-size: {{ block.settings.paragraph_size_mobile }}px;
      margin-bottom: {{ block.settings.paragraph_margin_bottom_mobile }}px;
    }

    .ai-floating-section__button-{{ ai_gen_id }} {
      {% if block.settings.button_full_width_mobile %}
      width: 100%;
      {% endif %}
    }

    /* Mobile image handling */
    .ai-floating-section__image-{{ ai_gen_id }} {
      {% if block.settings.image_mobile_behavior == 'hide' %}
      display: none;
      {% elsif block.settings.image_mobile_behavior == 'reduce' %}
      --fs-image-size: {{ block.settings.image_size_mobile }}px;
      opacity: {{ block.settings.image_mobile_opacity }};
      {% elsif block.settings.image_mobile_behavior == 'top' %}
      position: relative;
      top: auto;
      left: auto;
      right: auto;
      transform: none;
      margin: 0 auto {{ block.settings.image_mobile_margin }}px;
      --fs-image-size: {{ block.settings.image_size_mobile }}px;
      {% endif %}
    }

    {% if block.settings.image_mobile_behavior == 'top' %}
    .ai-floating-section__container-{{ ai_gen_id }} {
      display: flex;
      flex-direction: column;
      align-items: center;
    }

    .ai-floating-section__image-{{ ai_gen_id }} {
      order: -1;
    }
    {% endif %}
  }

  /* Reduced motion */
  @media (prefers-reduced-motion: reduce) {
    .ai-floating-section__image-{{ ai_gen_id }} {
      transition: none !important;
    }
  }
{% endstyle %}

<floating-section-{{ ai_gen_id }}
  class="ai-floating-section-{{ ai_gen_id }}"
  data-rotation-speed="{{ block.settings.rotation_speed }}"
  data-image-position="{{ block.settings.image_position }}"
  data-image-offset="{{ block.settings.image_offset }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-floating-section__container-{{ ai_gen_id }}">
    <div class="ai-floating-section__content-{{ ai_gen_id }}">
      {% if block.settings.title != blank %}
        <div class="ai-floating-section__title-{{ ai_gen_id }}">
          {{ block.settings.title }}
        </div>
      {% endif %}

      {% if block.settings.paragraph != blank %}
        <div class="ai-floating-section__paragraph-{{ ai_gen_id }}">
          {{ block.settings.paragraph }}
        </div>
      {% endif %}

      {% if block.settings.button_text != blank %}
        <a
          href="{{ block.settings.button_link | default: '#' }}"
          class="ai-floating-section__button-{{ ai_gen_id }}"
        >
          {{ block.settings.button_text }}
          {% if block.settings.button_show_arrow %}
            <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
              <line x1="5" y1="12" x2="19" y2="12"></line>
              <polyline points="12 5 19 12 12 19"></polyline>
            </svg>
          {% endif %}
        </a>
      {% endif %}
    </div>

    {%- comment -%} Floating Image {%- endcomment -%}
    <div class="ai-floating-section__image-{{ ai_gen_id }}">
      {% if block.settings.floating_image != blank %}
        <img
          src="{{ block.settings.floating_image | image_url: width: 800 }}"
          srcset="{{ block.settings.floating_image | image_url: width: 400 }} 400w,
                  {{ block.settings.floating_image | image_url: width: 600 }} 600w,
                  {{ block.settings.floating_image | image_url: width: 800 }} 800w"
          sizes="(max-width: 749px) {{ block.settings.image_size_mobile }}px, {{ block.settings.image_size }}px"
          alt="{{ block.settings.floating_image.alt | default: block.settings.title | strip_html | escape }}"
          loading="lazy"
          width="{{ block.settings.image_size }}"
          height="{{ block.settings.image_size }}"
        >
      {% else %}
        <div class="ai-floating-section__image-placeholder-{{ ai_gen_id }}">
          <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1">
            <rect x="3" y="3" width="18" height="18" rx="2" ry="2"/>
            <circle cx="8.5" cy="8.5" r="1.5"/>
            <polyline points="21 15 16 10 5 21"/>
          </svg>
        </div>
      {% endif %}
    </div>
  </div>
</floating-section-{{ ai_gen_id }}>

<script>
  (function() {
    if (customElements.get('floating-section-{{ ai_gen_id }}')) return;

    class FloatingSection extends HTMLElement {
      constructor() {
        super();
        this.image = null;
        this.rotationSpeed = 0.5;
        this.currentRotation = 0;
        this.lastScrollY = window.scrollY;
        this.imagePosition = 'right';
        this.imageOffset = 50;
        this.ticking = false;
      }

      connectedCallback() {
        this.image = this.querySelector('.ai-floating-section__image-{{ ai_gen_id }}');
        this.rotationSpeed = parseFloat(this.dataset.rotationSpeed) || 0.5;
        this.imagePosition = this.dataset.imagePosition || 'right';
        this.imageOffset = parseFloat(this.dataset.imageOffset) || 50;

        if (this.image && this.rotationSpeed > 0) {
          this.boundHandleScroll = this.handleScroll.bind(this);
          window.addEventListener('scroll', this.boundHandleScroll, { passive: true });
        }
      }

      disconnectedCallback() {
        if (this.boundHandleScroll) {
          window.removeEventListener('scroll', this.boundHandleScroll);
        }
      }

      handleScroll() {
        if (!this.ticking) {
          requestAnimationFrame(() => {
            this.updateRotation();
            this.ticking = false;
          });
          this.ticking = true;
        }
      }

      updateRotation() {
        if (!this.image) return;

        const currentScrollY = window.scrollY;
        const scrollDelta = currentScrollY - this.lastScrollY;
        
        this.currentRotation += scrollDelta * this.rotationSpeed;
        
        // Construir transform manteniendo la posición
        let baseTransform = 'translateY(-50%)';
        if (this.imagePosition === 'left') {
          baseTransform += ` translateX(-${this.imageOffset}%)`;
        } else {
          baseTransform += ` translateX(${this.imageOffset}%)`;
        }
        
        this.image.style.transform = `${baseTransform} rotate(${this.currentRotation}deg)`;
        this.lastScrollY = currentScrollY;
      }
    }

    customElements.define('floating-section-{{ ai_gen_id }}', FloatingSection);
  })();
</script>

